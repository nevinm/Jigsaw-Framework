@using Jigsaw.UI.Core.MasterCommon

@if (TempData["RenderStyles"] != null)
{ Html.RenderCommonStyles((bool)TempData["RenderStyles"]); }
else
{ Html.RenderCommonStyles(true); }

@if (TempData["RenderScripts"] != null)
{ Html.RenderCommonScripts((bool)TempData["RenderScripts"]); }
else
{ Html.RenderCommonScripts(true); }
